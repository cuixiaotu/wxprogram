page{
  background: #f5f5f5;
}
.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #fff;
  color: #939393;
}
.nav_left{
  display: inline-block;
  width: 25%;
  background: #f5f5f5;
  text-align: center;
}
.nav_left .nav_left_items{
  height: 60rpx;
  line-height: 60rpx;
  padding: 12rpx 0;
  border-bottom: 1px solid #dedede;
  font-size: 28rpx;
}
.nav_left .nav_left_items.active{
  background: #fff;
}
.nav_right{
  position: absolute;
  top: 0;
  right: 0;
  flex: 1;
  width: 75%;
  padding: 10px;
  background: #fff;
  box-sizing: border-box;
}
.nav_right .nav_right_items{
  float: left;
  width: 33.33%;
  height: 100rpx;
  text-align: center;
}
  /*图片已经屏蔽*/
.nav_right .nav_right_items image{
  width: 50px;
  height: 30px;
}


.nav_right .nav_right_items text{
  display: block;
  margin-top: 5px;
  font-size: 40rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}



